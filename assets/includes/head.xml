<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{% return site.name || site.title %}</title>
  <link rel="alternate" type="text/html" href="{{ site.url | canonical_url }}"/>
  <link rel="self" type="application/atom+xml" href="{{ page.url | canonical_url }}"/>
  <id>{{ site.url }}</id>
  <updated>{% feed_updated_date %}</updated>
  <generator uri="http://octopress.org/">Octopress</generator>
  <language>{{ site.lang }}</language>
  <rights>Copyright Â© {{ site.time | date: '%Y' }}, {{ site.author | strip_html }}</rights>
